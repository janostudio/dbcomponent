{
  "name": "dbcomponent",
  "version": "1.0.0",
  "description": "A component kit for dbapp's project.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js --open --inline --hot",
    "build": "webpack --config build/webpack.dist.js",
    "build:docs": "webpack --config build/webpack.docs.js",
    "build:lib": "webpack --config build/webpack.lib.js",
    "build:all": "npm run build && npm run build:lib && npm run build:docs",
    "clean": "rimraf ./dist ./lib ./docs/docs.bundle.css ./docs/docs.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janostudio/dbcomponent.git"
  },
  "keywords": [
    "component",
    "kit"
  ],
  "author": "JanathonJiang <qinghuangniao@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janostudio/dbcomponent/issues"
  },
  "homepage": "https://github.com/janostudio/dbcomponent#readme",
  "dependencies": {
    "deep-assign": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "3.0.2",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0"
  }
}
